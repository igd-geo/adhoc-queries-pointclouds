    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Debug)",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=io_tests",
                    "--package=io_tests"
                ],
                "filter": {
                    "name": "io_tests",
                    "kind": "bin"
                }
            },
            "args": [
                "-i",
                "/home/pbormann/data/geodata/pointclouds/NavVisHQ-5thFloor.laser",
                "--bounds",
                "-2;0;0;10;10;10",
                "-o"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "Launch (Release)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/release/io_tests.exe",
            "args": [
                "-i",
                "D:/data/geo/test_las",
                "--bounds",
                "360500;5628150;90;360550;5628200;130"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false
        }
    ]
}