    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Debug)",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=io_tests",
                    "--package=io_tests"
                ],
                "filter": {
                    "name": "io_tests",
                    "kind": "bin"
                }
            },
            "args": [
                "-i",
                "/home/pbormann/data/projects/progressive_indexing/experiments/file_formats/NavVisHQ-3rdFloor/navvis_m6_HQ3rdFloor.las",
                "--bounds",
                "-23.108;-21.261;-10.029;28.588;27.123;5.959" ,
                "--density",
                "0.1",
                "-o",
                "/home/pbormann/data/tmp"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "Launch (Release)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/release/io_tests.exe",
            "args": [
                "-i",
                "D:/data/geo/test_las",
                "--bounds",
                "360500;5628150;90;360550;5628200;130"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false
        }
    ]
}