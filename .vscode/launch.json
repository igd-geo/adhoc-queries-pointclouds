    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Debug)",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=example_query",
                ]
            },
            "args": [
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "info",
                "RUST_BACKTRACE": "1",
            },
        },
        {
            "name": "Launch (Release)",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=example_query",
                    "--release"
                ]
            },
            "args": [
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "info",
                "RUST_BACKTRACE": "1",
            },
        },
    ]
}